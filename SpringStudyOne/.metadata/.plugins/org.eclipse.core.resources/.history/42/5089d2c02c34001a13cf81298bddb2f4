package sandbox;

import java.io.*;
import java.util.ArrayList;
import java.util.Arrays;
import java.util.List; 
public class Main  
{
 	public static void main (String[] args) throws java.lang.Exception 
 	{

     	//use the following code to fetch input from console 
     	String line; 
     	BufferedReader inp = new BufferedReader (new InputStreamReader(System.in)); 
     	line=inp.readLine();
        
     	Integer arraySize = Integer.valueOf(line);
        int[] input = new int[arraySize];
        
        line=inp.readLine();     
        for (int i = 0; i < input.length; i++) {
            int num = Integer.parseInt((String.valueOf(line.toCharArray()[i])));
            input[i] = num;
        }
        line = Arrays.toString(input);
     	//Use the following code to print output
     	System.out.println(line); 
    }
}
